{
    "name": "solution-forest/inspirecms-support",
    "description": "This is my package inspirecms-support",
    "keywords": [
        "solution-forest",
        "laravel",
        "inspirecms-support"
    ],
    "homepage": "https://github.com/solutionforest/inspirecms-support",
    "support": {
        "issues": "https://github.com/solutionforest/inspirecms-support/issues",
        "source": "https://github.com/solutionforest/inspirecms-support"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "carly",
            "email": "info@solutionforest.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "filament/infolists": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "SolutionForest\\InspireCms\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolutionForest\\InspireCms\\Support\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolutionForest\\InspireCms\\Support\\InspireCmsSupportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}